{{#each notes}}
    <div class="wrap-collapsible">
        <input id="{{folder}}" class="toggle" type="checkbox">
        <label for="{{folder}}" class="title folder dropdown">{{folder}}</label>
        <div class="collapsible">

        {{#each note}}
                
                <div class="wrap-collabsible">
                    <input id="{{this.id}}" class="toggle" type="checkbox">
                    <label for="{{this.id}}" class="title note dropdown">{{this.name}}</label>
                    <div class="collapsible">
                        <div class="text-body content-inner">
                            <p>{{nl2br this.text}}</p>
                        </div>
                    </div>
                </div>

        {{/each}}

        </div>
    </div>
{{/each}}